<h1>New Song</h1>

<%flash[:message] && flash[:message].each do |m| %>
  <li><%=m%></li>
<%end%>


<form action="/songs" method="post">
  <label>Song Name: </label>
  <input type="text" name="song[name]">
  <label>Song Quantity: </label>
  <input type="text" name="song[quantity]">
  <input type="submit">
</form>
